        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#0c0900"
      },
      "package": "com.zopinos.reseptiforms",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "3f02d546-b483-45e7-9f30-3e718d8dd77c"
      }
